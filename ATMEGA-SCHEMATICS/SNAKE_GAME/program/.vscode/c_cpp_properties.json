{
  "version": 4,
  "configurations": [
    {
      "name": "macos-clang-arm64",
      "compilerPath": "/usr/bin/clang",
      "compilerArgs": [
        "-mmcu=atmega168",
        "-DF_CPU=8000000UL"
      ],
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "__AVR_ATmega168__"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "macos-clang-arm64"
    }
  ]
}